@model IEnumerable<gorodnitsevAutocompliterProj2.Models.NameInfo>
    
<br /><br />
@using(Html.BeginForm("Index","Home",FormMethod.Get))
{
    @Html.TextBox("searching")<input type="submit" value="Поиск" />

}
<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Имя</th>
            <th>Пол</th>
            <th>Иностранное имя</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="3" style="color:red">
                    ---
                </td>
            </tr>
        }
        else
        {
            foreach(var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Emakeelnenimi</td>
                    <td>@item.Sugu</td>
                    <td>@item.Voorkeelnenimi</td>
                </tr>
            }
        }
    </tbody>
</table>